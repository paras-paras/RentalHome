
@{
    ViewBag.Title = "AdminLogin";
}



<section class="why">
    <div class="container p-md-5 p-3">
        <h3 class="heading text-capitalize text-center">Rental Home </h3>
        <h5 class="heading mb-5 text-center">Contact Us</h5>
        <div class="row">
            <div class="col-md-6">
                <img src="~/images/admin.jpeg" class="img img-thumbnail" />
            </div>
            <div class="col-md-6">
                <form method="post" action="verifyCheck">
                    
                    <div class="form-group">
                        <label>Enter Email </label>
                        <input type="Email" class="form-control" id="LoginName" Name="LoginName" required />
                    </div>
                    <div class="form-group">
                        <label>Enter Password</label>
                        <input type="password" class="form-control" id="LoginPassword" Name="LoginPassword" required />
                    </div>
                    <input type="submit" class="btn btn-primary" />

                </form>
            </div>



        </div>
    </div>
</section>
